{
	"name": "DateDimDF",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DateDimSF",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DateDimSF",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          DATE as date,",
				"          DAY_OF_WEEK as string,",
				"          MONTH_NAME as string,",
				"          YEAR_NUM as decimal(4,0),",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DT as date,",
				"          DATE_DIM_SK as decimal(10,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> source1",
				"source1 keyGenerate(output(DATE_DIM_SK as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(DI_LOAD_DT = currentDate(),",
				"          DI_JOB_ID = uuid()) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          DATE,",
				"          DAY_OF_WEEK,",
				"          MONTH_NAME,",
				"          YEAR_NUM,",
				"          DATE_DIM_SK,",
				"          DI_LOAD_DT,",
				"          DI_JOB_ID",
				"     )) ~> sink1"
			]
		}
	}
}